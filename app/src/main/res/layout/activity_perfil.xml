<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.alberto.granvecino.Perfil">

    <TextView
        android:id="@+id/nombreTV"
        android:layout_width="151dp"
        android:layout_height="29dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/apellidosTV"
        android:layout_width="126dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/nombreTV"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/edadTV"
        android:layout_width="53dp"
        android:layout_height="25dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombreTV" />

    <TextView
        android:id="@+id/trabComTV"
        android:layout_width="72dp"
        android:layout_height="32dp"
        android:layout_marginStart="172dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/edadTV"
        app:layout_constraintTop_toBottomOf="@+id/apellidosTV" />
</android.support.constraint.ConstraintLayout>
